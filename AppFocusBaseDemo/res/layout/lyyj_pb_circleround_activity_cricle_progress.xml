<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:android_custom="http://schemas.android.com/apk/res/com.appfocusbase"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >


    <com.lyyj.activity.demo.progressbar.circleround.RoundProgressBar
        android:id="@+id/roundProgressBar2"
        android:layout_width="80dip"
        android:layout_height="80dip"
        android:layout_alignLeft="@+id/roundProgressBar1"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="78dp"
        
        
        android_custom:roundColorRPB="#D1D1D1"
        android_custom:roundProgressColorRPB="@android:color/black"
        android_custom:textColorRPB="#9A32CD"
        android_custom:roundWidthRPB="10dip"
        android_custom:textSizeRPB="18sp" />

    <com.lyyj.activity.demo.progressbar.circleround.RoundProgressBar
        android:id="@+id/roundProgressBar4"
        android_custom:styleRPB="FILLRPB"
        android:layout_width="80dip"
        android:layout_height="80dip"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/roundProgressBar1"
        android:layout_marginRight="32dp"
        android_custom:roundWidthRPB="1dip"
        android_custom:roundProgressColorRPB="#C2C2C2" />

    <com.lyyj.activity.demo.progressbar.circleround.RoundProgressBar
        android:id="@+id/roundProgressBar3"
        android:layout_width="80dip"
        android:layout_height="80dip"
        android:layout_alignLeft="@+id/roundProgressBar4"
        android:layout_alignTop="@+id/roundProgressBar2"
        android_custom:roundColorRPB="#C6E2FF"
        android_custom:roundWidthRPB="10dip"
        android_custom:roundProgressColorRPB="#CD3333"
        android_custom:textIsDisplayableRPB="false" />

    <com.lyyj.activity.demo.progressbar.circleround.RoundProgressBar
        android:id="@+id/roundProgressBar5"
        android:layout_width="50dip"
        android:layout_height="50dip"
        android:layout_below="@+id/roundProgressBar1"
        android:layout_marginLeft="22dp"
        android:layout_toRightOf="@+id/roundProgressBar1" />
    
    
        <Button
        android:id="@+id/button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:text="Button" />

    <com.lyyj.activity.demo.progressbar.circleround.RoundProgressBar
        android:id="@+id/roundProgressBar1"
        android:layout_width="80dip"
        android:layout_height="80dip"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/button1"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="40dp" />
    

</RelativeLayout>